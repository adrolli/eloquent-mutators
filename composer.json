{
    "name": "awobaz/eloquent-mutators",
    "description": "Reusable mutators (getters/setters) for Laravel 5's Eloquent",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel mutators",
        "laravel accessors",
        "laravel getters setters"
    ],
    "authors": [
        {"name": "Claudin J. Daniel", "email": "cdaniel@awobaz.com"}
    ],
    "require": {
      "php": ">=5.6.4",
      "illuminate/database": "~5.4",
      "illuminate/console": "~5.4",
      "illuminate/support": "~5.4",
      "illuminate/cache": "~5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "laravel/laravel": "~5.4",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Awobaz\\Mutator\\": "src"
        },
        "files": [

        ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "Awobaz\\Mutator\\MutatorServiceProvider"
        ]
      }
    },
    "suggest": {
      "awobaz/compoships": "Multi-columns relationships for Laravel 5's Eloquent",
      "awobaz/blade-active": "Blade directives for the Laravel 'Active' package",
      "awobaz/syntactic": "Syntactic sugar for named and indexed parameters call."
    }
}
